dy\n      </Link>{' '}\n      {new Date().getFullYear()}\n      {'.'}\n    </Typography>\n  );\n}\n\nconst useStyles = makeStyles((theme) => ({\n  paper: {\n    marginTop: theme.spacing(8),\n    display: 'flex',\n    flexDirection: 'column',\n    alignItems: 'center',\n  },\n  avatar: {\n    margin: theme.spacing(1),\n    backgroundColor: theme.palette.secondary.main,\n  },\n  form: {\n    width: '100%', // Fix IE 11 issue.\n    marginTop: theme.spacing(3),\n  },\n  submit: {\n    margin: theme.spacing(3, 0, 2),\n  },\n}));\n\nexport default function SignUp() {\n  const classes = useStyles();\n  const [formState, setFormState] = useState({ email: '', password: '' });\n  const [addUser, { error, data }] = useMutation(ADD_USER);\n\n  const handleFormSubmit = async (event) => {\n    event.preventDefault();\n    try {\n      const { data } = await addUser({\n        variables: { ...formState },\n      });\n\n      Auth.login(data.addUser.token);\n    } catch (e) {\n      console.error(e);\n    }\n  };\n\n  const handleChange = (event) => {\n    const { name, value } = event.target;\n    setFormState({\n      ...formState,\n      [name]: value,\n    });\n  };\n\n  return (\n    <Container component='main' maxWidth='xs'>\n      <CssBaseline />\n      <div className={classes.paper}>\n        <Avatar className={classes.avatar}>\n          <LockOutlinedIcon />\n        </Avatar>\n        <Typography component='h1' variant='h5'>\n          Sign up\n        </Typography>\n        <form className={classes.form} noValidate onSubmit={handleFormSubmit}>\n          <Grid container spacing={2}>\n            <Grid item xs={12} sm={6}>\n              <TextField\n                autoComplete='fname'\n                name='firstName'\n                variant='outlined'\n                required\n                fullWidth\n                id='firstName'\n                label='First Name'\n                autoFocus\n                onChange={handleChange}\n              />\n            </Grid>\n            <Grid item xs={12} sm={6}>\n              <TextField\n                variant='outlined'\n                required\n                fullWidth\n                id='lastName'\n                label='Last Name'\n                name='lastName'\n                autoComplete='lname'\n                onChange={handleChange}\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <TextField\n                variant='outlined'\n                required\n                fullWidth\n                id='email'\n                label='Email Address'\n                name='email'\n                autoComplete='email'\n                onChange={handleChange}\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <TextField\n                variant='outlined'\n                required\n                fullWidth\n                name='password'\n                label='Password'\n                type='password'\n                id='password'\n                autoComplete='current-password'\n                onChange={handleChange}\n              />\n            </Grid>\n            <Grid item xs={12}>\n              <FormControlLabel\n                control={<Checkbox value='allowExtraEmails' color='primary' />}\n                label='I want to receive inspiration, marketing promotions and updates via email.'\n              />\n            </Grid>\n          </Grid>\n          <Button\n            type='submit'\n            fullWidth\n            variant='contained'\n            color='primary'\n            className={classes.submit}\n          >\n            Sign Up\n          </Button>\n          <Grid container justifyContent='flex-end'>\n            <Grid item>\n              <Link href='#' variant='body2'>\n                Already have an account? Sign in\n              </Link>\n            </Grid>\n          </Grid>\n        </form>\n      </div>\n      <Box mt={5}>\n        <Copyright />\n      </Box>\n    </Container>\n  );\n}\n","/Users/hugh/Documents/College/Berkeley Boot Camp/Project 3/Tidy/tidy/client/src/utils/mutations.js",[],{"ruleId":"101","replacedBy":"102"},{"ruleId":"103","replacedBy":"104"},{"ruleId":"105","severity":1,"message":"106","line":31,"column":3,"nodeType":"107","messageId":"108","endLine":31,"endColumn":13},{"ruleId":"105","severity":1,"message":"109","line":32,"column":3,"nodeType":"107","messageId":"108","endLine":32,"endColumn":9},{"ruleId":"105","severity":1,"message":"110","line":33,"column":3,"nodeType":"107","messageId":"108","endLine":33,"endColumn":6},{"ruleId":"105","severity":1,"message":"111","line":35,"column":3,"nodeType":"107","messageId":"108","endLine":35,"endColumn":7},{"ruleId":"105","severity":1,"message":"112","line":36,"column":3,"nodeType":"107","messageId":"108","endLine":36,"endColumn":14},{"ruleId":"105","severity":1,"message":"113","line":37,"column":3,"nodeType":"107","messageId":"108","endLine":37,"endColumn":14},{"ruleId":"105","severity":1,"message":"114","line":38,"column":3,"nodeType":"107","messageId":"108","endLine":38,"endColumn":12},{"ruleId":"105","severity":1,"message":"115","line":39,"column":3,"nodeType":"107","messageId":"108","endLine":39,"endColumn":14},{"ruleId":"105","severity":1,"message":"116","line":40,"column":3,"nodeType":"107","messageId":"108","endLine":40,"endColumn":7},{"ruleId":"105","severity":1,"message":"117","line":41,"column":3,"nodeType":"107","messageId":"108","endLine":41,"endColumn":7},{"ruleId":"105","severity":1,"message":"118","line":42,"column":3,"nodeType":"107","messageId":"108","endLine":42,"endColumn":8},{"ruleId":"105","severity":1,"message":"119","line":43,"column":3,"nodeType":"107","messageId":"108","endLine":43,"endColumn":10},{"ruleId":"105","severity":1,"message":"120","line":44,"column":3,"nodeType":"107","messageId":"108","endLine":44,"endColumn":12},{"ruleId":"105","severity":1,"message":"121","line":48,"column":10,"nodeType":"107","messageId":"108",